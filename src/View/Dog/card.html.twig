<div class="col-sm-6 col-md-4 col-lg-3">
    <div class="card text-center m-3">
        <img class="card-img-top rounded-circle mt-3 mx-auto" src="{{ dog.picture }}" alt="image du chien">
        <div class="card-body">
            <h5 class="card-title font-weight-bold">{{ dog.name }}</h5>
            <p class="card-text">{{ dog.birthday }}</p>
            <p class="card-text">{{ dog.price }}</p>
            <a href="#" class="btn btn-primary">En Savoir Plus</a>
        </div>
    </div>
</div>